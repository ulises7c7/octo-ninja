      </div>




<div class="row">
        <div class="col-sm-6">
          <div class="panel panel-default">
            <div class="panel-body">


<table class="table table-condensed table-hover">
<thead>
<tr>
	<th>Codigo</th>
	<th>Nombre</th>
	<th>Acciones</th>
</tr>
</thead>
<tbody>
{% for vehiculo in vehiculos %}

