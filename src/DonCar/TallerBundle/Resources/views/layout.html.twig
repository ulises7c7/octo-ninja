    </div>

    <div class="search">
      <h1 style="font-size:50px;">Taller</h1>
    </div>

  </div>

  <div class="sf-reset">

    <div class="block">
    <ul>
      <li><a href="{{ path( 'taller_listar_orden') }}">Ordenes</a></li>
      <li><a href="{{ path( 'taller_listar_mecanico') }}">Mecanicos</a></li>
      <li><a href="{{ path( 'taller_gestion_orden') }}">Iniciar/Detener Trabajo</a></li>
    </ul>
 
      {% block content %}
      {% endblock %}  

    </div>
  </div>
</div>

{% endblock %}
